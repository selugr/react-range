{
    "presets": [
        // "@babel/preset-env",
        ["@babel/preset-env", {
            "targets": 
                {
                    "node": "current"
                }
            }
        ],
        [
            "@babel/preset-react", {
                "runtime": "automatic"
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-syntax-dynamic-import",
        "@babel/plugin-transform-runtime"
    ]
}